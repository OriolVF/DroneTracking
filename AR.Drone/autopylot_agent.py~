
import imp
#from DistanceError import *
algorithm = "TLD" # or DPM
# Routine called by C program.
def action(img_bytes, img_width, img_height, is_belly, ctrl_state, vbat_flying_percentage, theta, phi, psi, altitude, vx, vy, vz):

	imp.load_source('Mod','../Python/Mod.py')
	imp.load_source('DistanceError','../Python/DistanceError.py')
	#imp.load_source('DroneCommands','../Python/DroneCommands.py')
	imp.load_source('SaveImage','../Python/SaveImage.py')
	if (algorithm == "TLD"):
		InterfaceTLD = imp.load_source('InterfaceTLD','../Python/interfaceTLD.py')
		InterfaceTLD.action(img_bytes, img_width, img_height, is_belly, ctrl_state, vbat_flying_percentage, theta, phi, psi, altitude, vx, vy, vz)
	elif (algorithm == "DPM"):
		imp.load_source('InterfaceDPM','../Python/interfaceDPM.py')
		interfaceDPM.action(img_bytes, img_width, img_height, is_belly, ctrl_state, vbat_flying_percentage, theta, phi, psi, altitude, vx, vy, vz)
